# LangSmith (observability)
# Set to true to send traces to LangSmith. Required for multi-agent debugging.
LANGCHAIN_TRACING_V2=true
# API key from https://smith.langchain.com (or your LangSmith host)
LANGCHAIN_API_KEY=

# Groq (Llama 3.1 70B) — Judges and optional RepoInvestigator LLM
GROQ_API_KEY=

# Google AI Studio (Gemini 1.5 Flash) — DocAnalyst and VisionInspector
GOOGLE_API_KEY=

# Optional override for local testing (default stack does not require OpenAI)
# OPENAI_API_KEY=
